{
    "name": "your-vendor/php-canvas-lms-api-orm",
    "description": "A PHP library that provides an ORM for the Canvas LMS API",
    "type": "library",
    "license": "All-rights-reserved",
    "autoload": {
        "psr-4": {
            "CanvasApiLibrary\\": "./",
            "CanvasApiLibrary\\Models\\": [
                "models/",
                "models/generated/",
                "models/context-population-traits/",
                "models/utility/"
            ],
            "CanvasApiLibrary\\Models\\ContextPopulationTraits\\": [
                "models/context-population-traits/"
            ],
            "CanvasApiLibrary\\Providers\\": [
                "providers/",
                "providers/generated/",
                "providers/utility/",
                "providers/utility/model-populator/",
                "providers/utility/model-populator/interfaces/",
                "providers/utility/model-populator/implementations/"
            ],
            "CanvasApiLibrary\\Services\\": [
                "services/"
            ],
            "CanvasApiLibrary\\Exceptions\\": [
                "exceptions/"
            ]
        }
    },
    "require": {
        "php": ">=8.4"
    },
    "scripts": {
        "serve": "php -S localhost:8000 test.php"
    }
}
